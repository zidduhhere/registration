# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_PUBLISHABLE_KEY=your_anon_key

# Admin Configuration
VITE_ADMIN_PASSWORD=your_admin_password

# Debug Mode Configuration (for delete functionality)
# Set to 'true' to enable debug mode and delete functionality in admin dashboard
VITE_DEBUG_MODE=false

# Supabase Service Key (Required for debug mode delete functionality)
# WARNING: Keep this secret! Never commit this to version control
# Only use in development/testing environments
VITE_SUPABASE_SERVICE_KEY=your_service_role_key
